{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-import-module-exports": "off",
        "react/jsx-no-undef": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            1, {
            "extensions": [".js", ".jsx"]
            }
        ],
        "singleQuote": 0
    }
}
